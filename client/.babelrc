{
  "presets": ["react", "stage-1", ["env", { "modules": false }]],
  "plugins": [
    "react-hot-loader/babel",
    "babel-plugin-transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["react", "stage-1", ["env", { "modules": "commonjs" }]],
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
